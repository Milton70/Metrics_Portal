<div class="container">    
	<div class="col-md-10 col-md-offset-1">
	  <div class="panel panel-default panel-table">
	    <div class="panel-heading">
	      <div class="row">
	        <div class="col col-xs-6">
	          <h3 class="panel-title">Rest Service</h3>
	        </div>
	      </div>
	    </div>
	    <div class="panel-body">
	    	<div class="col col-xs-3" data-turbolinks="false">
	        <%= link_to "Get All Employees", rest_service_path(id: 'all'), class: "btn btn-sm btn-primary" %>
	      </div>
	      <div class="col col-xs-3" data-turbolinks="false">
	        <%= link_to "Get One Employee", get_an_employee_path(id: 166), class: "btn btn-sm btn-info" %>
	      </div>
	      <div class="col col-xs-3" data-turbolinks="false">
	        <%= link_to "Edit One Employee", edit_rest_service_path(id: 166, name: "mik", salary: "123", age: "18"), class: "btn btn-sm btn-warning" %>
	      </div>
	      <div class="col col-xs-3" data-turbolinks="false">
	        <%= link_to "Delete One Employee", rest_service_path(id: 180), method: :delete, data: {confirm: 'Are you sure?'}, class: "btn btn-danger" %>
	      </div>
	    </div>
	    <div class="panel-footer">
	     	<%= @json_response %>
	    </div>
	  </div>
	</div>
</div>